	--color-secondary-100: 223 231 248; /* ⬅ #dfe7f8 */
	--color-secondary-200: 215 225 246; /* ⬅ #d7e1f6 */
	--color-secondary-300: 191 206 241; /* ⬅ #bfcef1 */
	--color-secondary-400: 143 170 230; /* ⬅ #8faae6 */
	--color-secondary-500: 95 133 219; /* ⬅ #5F85DB */
