:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: Inter, ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont,
		'Segoe UI', Roboto, 'Helvetica Neue', Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji',
		'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji';
	--theme-font-family-heading: Inter, ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont,
		'Segoe UI', Roboto, 'Helvetica Neue', Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji',
		'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji';
	--theme-font-color-base: var(--color-surface-900);
	--theme-font-color-dark: var(--color-surface-50);
	--theme-rounded-base: 12px;
	--theme-rounded-container: 16px;
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: var(--color-surface-900);
	--on-secondary: var(--color-surface-900);
	--on-tertiary: var(--color-surface-50);
	--on-success: var(--color-surface-900);
	--on-warning: var(--color-surface-900);
	--on-error: var(--color-surface-900);
	--on-surface: var(--color-primary-50);
	/* =~= Theme Colors  =~= */
	/* primary | #90B8F8 */
	--color-primary-50: 238 244 254; /* ⬅ #eef4fe */
	--color-primary-100: 233 241 254; /* ⬅ #e9f1fe */
	--color-primary-200: 227 237 253; /* ⬅ #e3edfd */
	--color-primary-300: 211 227 252; /* ⬅ #d3e3fc */
	--color-primary-400: 177 205 250; /* ⬅ #b1cdfa */
	--color-primary-500: 144 184 248; /* ⬅ #90B8F8 */
	--color-primary-600: 130 166 223; /* ⬅ #82a6df */
	--color-primary-700: 108 138 186; /* ⬅ #6c8aba */
	--color-primary-800: 86 110 149; /* ⬅ #566e95 */
	--color-primary-900: 71 90 122; /* ⬅ #475a7a */
	/* secondary | #5F85DB */
