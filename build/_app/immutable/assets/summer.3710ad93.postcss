:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: Inter, ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont,
		'Segoe UI', Roboto, 'Helvetica Neue', Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji',
		'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji';
	--theme-font-family-heading: Inter, ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont,
		'Segoe UI', Roboto, 'Helvetica Neue', Arial, 'Noto Sans', sans-serif, 'Apple Color Emoji',
		'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji';
	--theme-font-color-base: var(--color-surface-900);
	--theme-font-color-dark: var(--color-surface-50);
	--theme-rounded-base: 12px;
	--theme-rounded-container: 16px;
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: 0 0 0;
	--on-secondary: 0 0 0;
	--on-tertiary: 0 0 0;
	--on-success: 0 0 0;
	--on-warning: 0 0 0;
	--on-error: 0 0 0;
	--on-surface: 0 0 0;
	/* =~= Theme Colors  =~= */
	/* primary | #FFEEBB */
	--color-primary-50: 255 252 245; /* ⬅ #fffcf5 */
	--color-primary-100: 255 252 241; /* ⬅ #fffcf1 */
	--color-primary-200: 255 251 238; /* ⬅ #fffbee */
	--color-primary-300: 255 248 228; /* ⬅ #fff8e4 */
	--color-primary-400: 255 243 207; /* ⬅ #fff3cf */
	--color-primary-500: 255 238 187; /* ⬅ #FFEEBB */
	--color-primary-600: 230 214 168; /* ⬅ #e6d6a8 */
	--color-primary-700: 191 179 140; /* ⬅ #bfb38c */
	--color-primary-800: 153 143 112; /* ⬅ #998f70 */
	--color-primary-900: 125 117 92; /* ⬅ #7d755c */
	/* secondary | #A7ECEE */
	--color-secondary-50: 242 252 252; /* ⬅ #f2fcfc */
	--color-secondary-100: 237 251 252; /* ⬅ #edfbfc */
	--color-secondary-200: 233 250 251; /* ⬅ #e9fafb */
	--color-secondary-300: 220 247 248; /* ⬅ #dcf7f8 */
	--color-secondary-400: 193 242 243; /* ⬅ #c1f2f3 */
	--color-secondary-500: 167 236 238; /* ⬅ #A7ECEE */
	--color-secondary-600: 150 212 214; /* ⬅ #96d4d6 */
	--color-secondary-700: 125 177 179; /* ⬅ #7db1b3 */
